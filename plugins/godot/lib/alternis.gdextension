[configuration]

entry_symbol = "alternis_library_init"
compatibility_minimum = "4.1"

[libraries]

macos.debug = "res://lib/libalternis-godot.macos.template_debug.framework"
macos.release = "res://lib/libalternis-godot.macos.template_release.framework"
windows.debug.x86_32 = "res://lib/libalternis-godot.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "res://lib/libalternis-godot.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "res://lib/libalternis-godot.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://lib/libalternis-godot.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://lib/libalternis-godot.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://lib/libalternis-godot.linux.template_release.x86_64.so"
linux.debug.arm64 = "res://lib/libalternis-godot.linux.template_debug.arm64.so"
linux.release.arm64 = "res://lib/libalternis-godot.linux.template_release.arm64.so"
linux.debug.rv64 = "res://lib/libalternis-godot.linux.template_debug.rv64.so"
